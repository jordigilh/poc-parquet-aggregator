# OCP Parquet Aggregator POC - Environment Variables
# Copy this file to .env and fill in the values

# S3/MinIO Configuration
S3_ENDPOINT=https://s3-openshift-storage.apps.cluster.example.com
S3_ACCESS_KEY=your_access_key_here
S3_SECRET_KEY=your_secret_key_here
S3_BUCKET=cost-management

# PostgreSQL Configuration
POSTGRES_HOST=postgresql.cost-management.svc.cluster.local
POSTGRES_DB=koku
POSTGRES_USER=koku
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_SCHEMA=org1234567

# OCP Provider Configuration
OCP_PROVIDER_UUID=00000000-0000-0000-0000-000000000001
OCP_CLUSTER_ID=test-cluster-001
OCP_CLUSTER_ALIAS=Test OCP Cluster
OCP_YEAR=2025
OCP_MONTH=11
OCP_START_DATE=2025-11-01
OCP_END_DATE=2025-11-30

# Logging
LOG_LEVEL=INFO

# Trino (for validation only)
TRINO_HOST=trino-coordinator.cost-management.svc.cluster.local

